{
    "manifest_version": 2,
    "name": "Bookmark Manager",
    "version": "0.0.1",
    "description": "the bookmark manager like Google's ",
    "icons": {
        "16": "images/star_16x16.png",
        "48": "images/star_48x48.png",
        "128": "images/star_128x128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/star_19x19.png",
            "38": "images/star_38x38.png"
        },
        "default_title": "Bookmark Manager",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": ["*://www.google.com/*"],
    "web_accessible_resources": ["images/*.png"]
}